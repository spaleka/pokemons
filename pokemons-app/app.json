{
  "expo": {
    "name": "pokemons-app",
    "slug": "pokemons-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pokemonsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pokemonsapp",
      "config": {
        "googleMapsApiKey": "AIzaSyAIXgl0smEJdd4TX0hs_UezB3m4G4LPgwo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.pokemonsapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAIXgl0smEJdd4TX0hs_UezB3m4G4LPgwo"
        }
      }
    },
    "plugins": [
      "expo-maps"
    ]
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "8761943f-da72-4f0f-b500-eb8ad8202595"
    }
  }
}
